proc setup {} {
    save_env
}


proc teardown {} {
    assert_env_unmodified
}


setup


assert_no_complete "jq "
sync_after_int

assert_complete_any "jq . "
sync_after_int

assert_complete_any "jq -"
sync_after_int

assert_no_complete "jq --arg "
sync_after_int

assert_no_complete "jq --slurpfile "
sync_after_int

assert_complete_any "jq --slurpfile foo "
sync_after_int


teardown
